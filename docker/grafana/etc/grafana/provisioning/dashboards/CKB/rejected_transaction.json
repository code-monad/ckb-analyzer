          "metricColumn": "none",
          "rawQuery": true,
          "rawSql": "WITH v1 AS (\n    SELECT\n        time_bucket(INTERVAL '$interval', bt.time) AS \"rejected_time\",\n        EXTRACT(MILLISECOND FROM (bt.time - snt.time)) AS \"seconds\"\n    FROM ckb.subscribed_rejected_transaction bt JOIN ckb.subscribed_new_transaction snt\n    ON $__timeFilter(bt.time) AND bt.proposal_id = snt.proposal_id\n)\n\nSELECT\n  v1.rejected_time AS time,\n  avg(v1.seconds) AS \"Pending to Rejected\"\nFROM v1\nGROUP BY v1.rejected_time\nORDER BY v1.rejected_time;",
          "refId": "A",
